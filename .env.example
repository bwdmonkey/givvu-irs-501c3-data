# Google Cloud Platform
GCP_PROJECT_ID=your-gcp-project-id
GCP_LOCATION=US

# BigQuery
BQ_DATASET=irs_501c3

# Google Cloud Storage (staging bucket for BigQuery loads)
GCS_BUCKET=your-gcs-bucket-name

# Google Application Credentials (path to service account JSON key)
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Pipeline settings
DATA_DIR=./data
# Number of concurrent XML downloads
XML_DOWNLOAD_CONCURRENCY=50
# Tax years to process (comma-separated, most recent 3)
TAX_YEARS=2022,2023,2024
